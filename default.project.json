{
  "name": "pillarworks sourcemap",

  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      
      "packages": {
        "$ignoreUnknownInstances": true,
        
        "$path": {
          "optional": "roblox_packages"
        }
      },

      "shared": {
        "$ignoreUnknownInstances": true,
        
        "$path": {
          "optional": "src/shared"
        }
      },

      "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "server": {
        "$ignoreUnknownInstances": true,
        "$path": {
          "optional": "src/server"
        }
      },

      "$ignoreUnknownInstances": true
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$ignoreUnknownInstances": true,
        
        "client": {
          "$ignoreUnknownInstances": true,
          "$path": {
            "optional": "src/client"
          }
        }
      },

      "$ignoreUnknownInstances": true
    }
  }
}
